<tr id='note_row_id<%= note.id %>'>
  <td><%= note.id %></td>
  <td><%= note.title %></td>
  <td><%= truncate(note.content, length: 80) %></td>
  <td><%= time_ago_in_words(note.created_at) %> ago</td>
  <td><%= time_ago_in_words(note.updated_at) %> ago</td>
	<td>
    <%= link_to "", user_note_path, method: :edit, class: 'icon-edit', title: 'Edit Note' %>  

    <%= link_to "", user_note_path, method: :delete, class: 'icon-trash', title: 'Delete Note',
                confirm: "Are you sure that you want to delete Note ##{note.id}?", remote: true %>

  </td>
 <tr>